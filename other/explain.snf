# https://www.postgresql.org/docs/17/sql-explain.html

EXPLAIN [ ( config ) ] sql_statement

# WHERE config
[ ANALYZE [ analyze ] ]
[ VERBOSE [ verbose ] ]
[ COSTS [ costs ] ]
[ SETTINGS [ settings ] ]
[ GENERIC_PLAN [ generic_plan ] ]
[ BUFFERS [ buffers ] ]
[ SERIALIZE [ { NONE | TEXT | BINARY } ] ]
[ WAL [ wal ] ]
[ TIMING [ timing ] ]
[ SUMMARY [ summary ] ]
[ MEMORY [ memory ] ]
[ FORMAT { TEXT | XML | JSON | YAML } ]