# https://www.postgresql.org/docs/17/sql-creatematerializedview.html

CREATE MATERIALIZED VIEW [ IF NOT EXISTS ] name [ (colname [, ...] ) ]
    [ USING index_method ]
    [ WITH ( { parameter [= value] } [, ...] ) ]
    [ TABLESPACE tablespace ]
    AS select_statement
    [ WITH [ NO ] DATA ]

# STATEMEMT select_statement
