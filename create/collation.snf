# https://www.postgresql.org/docs/17/sql-createcollation.html

CREATE COLLATION [ IF NOT EXISTS ] name (
    [ LOCALE = locale, ]
    [ LC_COLLATE = lc_collate, ]
    [ LC_CTYPE = lc_ctype, ]
    [ PROVIDER = provider, ]
    [ DETERMINISTIC = deterministic, ]
    [ RULES = rules, ]
    [ VERSION = version ]
)

CREATE COLLATION [ IF NOT EXISTS ] name FROM collation