# https://www.postgresql.org/docs/17/sql-createtableas.html

CREATE [ GLOBAL | LOCAL | TEMPORARY | TEMP | UNLOGGED ] TABLE [ IF NOT EXISTS ] name
    [ ( colname [, ...] ) ]
    [ USING index_method ]
    [ WITH ( { parameter [= value] } [, ...] ) ]
    [ WITHOUT OIDS ]
    [ ON COMMIT { PRESERVE ROWS | DELETE ROWS | DROP } ]
    [ TABLESPACE tablespace ]
    AS select_statement
    [ WITH [ NO ] DATA ]

# STATEMEMT select_statement
SELECT
TABLE
VALUES
EXECUTE