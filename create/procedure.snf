# https://www.postgresql.org/docs/17/sql-createprocedure.html

CREATE [ OR REPLACE ] PROCEDURE name ( arg [, ...] )
    [ LANGUAGE language ]
    [ TRANSFORM { FOR TYPE type } [, ...] ]
    [ [ EXTERNAL ] SECURITY { INVOKER | DEFINER } ]
    [ SET parameter { TO value | = value | FROM CURRENT } [, ...] ]
    [ AS 'file', 'function' ]
    [ AS pg_definition ]
    [ sql_definition ]

# WHERE arg
[ argmode ] [ argname ] argtype [ { DEFAULT | = } value_expression ]