# https://www.postgresql.org/docs/17/sql-altertablespace.html

ALTER TABLESPACE name RENAME TO new_name

ALTER TABLESPACE name SET ( { tablespace_parameter [= value ] } [, ...] )

ALTER TABLESPACE name RESET ( tablespace_parameter [, ...] )

ALTER TABLESPACE name OWNER TO role

# WHERE role
name | CURRENT_ROLE | CURRENT_USER | SESSION_USER