# https://www.postgresql.org/docs/17/sql-alterforeigndatawrapper.html

ALTER FOREIGN DATA WRAPPER name
    [ HANDLER handler | NO HANDLER ]
    [ VALIDATOR validator | NO VALIDATOR ]
    [ OPTIONS ( option [, ...]) ]

ALTER FOREIGN DATA WRAPPER name RENAME TO new_name

ALTER FOREIGN DATA WRAPPER name OWNER TO role

# WHERE option
[ ADD | SET | DROP ] name ['value']

# WHERE role
name | CURRENT_ROLE | CURRENT_USER | SESSION_USER