# https://www.postgresql.org/docs/17/sql-alterserver.html

ALTER SERVER name [ VERSION 'version' ] [ OPTIONS ( option_item [, ...] ) ]

ALTER SERVER name RENAME TO new_name

ALTER SERVER name OWNER TO role

# WHERE option_item
[ ADD | SET | DROP ] option ['value']

# WHERE role
name | CURRENT_ROLE | CURRENT_USER | SESSION_USER