# https://www.postgresql.org/docs/17/sql-alterdomain.html

ALTER DOMAIN name ADD constraint [ NOT VALID ]

ALTER DOMAIN name DROP DEFAULT

ALTER DOMAIN name DROP NOT NULL

ALTER DOMAIN name DROP CONSTRAINT [ IF EXISTS ] consname [ RESTRICT | CASCADE ]

ALTER DOMAIN name RENAME CONSTRAINT consname TO new_consname

ALTER DOMAIN name RENAME TO new_name

ALTER DOMAIN name VALIDATE CONSTRAINT consname

ALTER DOMAIN name SET DEFAULT default

ALTER DOMAIN name SET NOT NULL

ALTER DOMAIN name SET SCHEMA schema

ALTER DOMAIN name OWNER TO { owner | CURRENT_ROLE | CURRENT_USER | SESSION_USER }

# where constraint
[ CONSTRAINT consname ] { NOT NULL | CHECK (check) }